map MP-Ded-tot -title "Synthèse topographie du Réseau Ded"
	
    MP-Kriska-tot@Kriska
    break
    MP-Valombre-plan-tot@Valombre [0 -60 m] below
    break
	MP-Aura-plan-tot@Aura
    break
    MP-Corneilles-plan-tot@Corneilles
    break
	MP-FT-plan-tot@FT [0 -60 m] below
    break
    MP-Puits1-coupe-tot@Puits1
    break
    MP-William-plan-tot@William
    break
    MP-Vire-plan-tot@Vire
    break
    MP-SE-plan-tot@SE-tot
    

endmap

map MC-Ded-tot -title "Synthèse topographie du Réseau Ded"
	
    MC-Kriska-tot-elev@Kriska
    break
    MP-Valombre-coupe-tot@Valombre 
    break
	MC-Aura-coupe-tot@Aura [-50 0 m] below
    break
    MC-Corneilles-coupe-tot@Corneilles
    break
	MC-FT-coupe-tot@FT  #[150 0 m] below
    break
    MC-Puits1-coupe-tot@Puits1
    break
    MC-William-coupe-tot@William [-50 0 m] below
    break
    MC-Vire-coupe-tot@Vire
    break
    MC-SE-elevation-tot@SE-tot
    

endmap
